<section class="rgen-panel">
	<h2 class="hd">Normal</h2>
	<div class="panel-container">
		<div class="rgen-form-wrp form-full form-medium">
			<rgen-box label="Block settings" config="{fonts: 0}" buttonlabel="Edit" data-ng-model="themeData.megamenu.normalmenu.block"></rgen-box>
			
			<div class="field-box">
				<label>Title and Sub links</label>
				<div class="fields">
					<ul class="form-row">
						<li>
							<label class="lbl">Title</label>
							<rgen-button type="alone" config="{align:1, f_size:1}" buttonlabel="Edit" data-ng-model="themeData.megamenu.normalmenu.heading"></rgen-button>
						</li>
						<li>
							<label class="lbl">Sub links</label>
							<rgen-button type="alone" config="{align:1, f_size:1}" buttonlabel="Edit" data-ng-model="themeData.megamenu.normalmenu.links"></rgen-button>
						</li>
					</ul>
				</div>
			</div>

		</div>
	</div>
</section>