<rgen-reset resetfn="reset('buttons')" data-ng-model="themeData.status"></rgen-reset>

<section class="rgen-panel">
	<h2 class="hd">Buttons</h2>
	<div class="panel-container">
		<div class="rgen-form-wrp form-full form-medium">
			<rgen-button label="Default button" buttonlabel="Edit" data-ng-model="themeData.default_btn"></rgen-button>
			<rgen-button label="Primary button" buttonlabel="Edit" data-ng-model="themeData.primary_btn"></rgen-button>
			<rgen-button label="Small button" buttonlabel="Edit" data-ng-model="themeData.small_btn"></rgen-button>
			<rgen-button label="Mini button" buttonlabel="Edit" data-ng-model="themeData.mini_btn"></rgen-button>
			<rgen-button label="Extra small button" buttonlabel="Edit" data-ng-model="themeData.xs_btn"></rgen-button>
		</div>
	</div>
</section>